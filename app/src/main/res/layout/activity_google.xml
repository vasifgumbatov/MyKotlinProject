<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GoogleActivity">

    <com.google.android.material.textview.MaterialTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/AccountCreated"
        android:layout_marginTop="300dp"
        android:layout_marginStart="159dp"
        android:layout_marginEnd="160dp"
        android:text="@string/google"
        android:textSize="30sp"
        android:textAlignment="center"
        android:textColor="@color/create_account"
        android:fontFamily="@font/poppins_font"
        tools:ignore="MissingConstraints">

    </com.google.android.material.textview.MaterialTextView>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/google_back_button"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:text="@string/account_created_button_text"
        android:textSize="20sp"
        android:fontFamily="@font/poppins_font"
        android:backgroundTint="@color/create_account"
        style="@style/MyButtonConstraints"
        android:layout_marginBottom="76dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:cornerRadius="50dp"
        tools:ignore="MissingConstraints">

    </com.google.android.material.button.MaterialButton>

</androidx.constraintlayout.widget.ConstraintLayout>